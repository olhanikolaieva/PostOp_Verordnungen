definition
	OBSERVATION[at0000] matches {    -- Birth Satisfaction Scale-Revised (BSS-R)
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {    -- 1. Unscathed childbirth
										value matches {
											4.0|[local::at0049],
											3.0|[local::at0050],
											2.0|[local::at0051],
											1.0|[local::at0052],
											0.0|[local::at0053]
										}
									}
									ELEMENT[at0010] occurrences matches {0..1} matches {    -- 2. Long labour
										value matches {
											0.0|[local::at0085],
											1.0|[local::at0086],
											2.0|[local::at0087],
											3.0|[local::at0088],
											4.0|[local::at0089]
										}
									}
									ELEMENT[at0016] occurrences matches {0..1} matches {    -- 3. Feel in charge of labour
										value matches {
											4.0|[local::at0070],
											3.0|[local::at0071],
											2.0|[local::at0072],
											1.0|[local::at0073],
											0.0|[local::at0074]
										}
									}
									ELEMENT[at0017] occurrences matches {0..1} matches {    -- 4. Anxiety
										value matches {
											0.0|[local::at0090],
											1.0|[local::at0091],
											2.0|[local::at0092],
											3.0|[local::at0093],
											4.0|[local::at0094]
										}
									}
									ELEMENT[at0018] occurrences matches {0..1} matches {    -- 5. Sufficient support
										value matches {
											4.0|[local::at0065],
											3.0|[local::at0066],
											2.0|[local::at0067],
											1.0|[local::at0068],
											0.0|[local::at0069]
										}
									}
									ELEMENT[at0019] occurrences matches {0..1} matches {    -- 6. Staff Communication
										value matches {
											4.0|[local::at0060],
											3.0|[local::at0061],
											2.0|[local::at0062],
											1.0|[local::at0063],
											0.0|[local::at0064]
										}
									}
									ELEMENT[at0020] occurrences matches {0..1} matches {    -- 7. Distressing experience
										value matches {
											0.0|[local::at0095],
											1.0|[local::at0096],
											2.0|[local::at0097],
											3.0|[local::at0098],
											4.0|[local::at0099]
										}
									}
									ELEMENT[at0021] occurrences matches {0..1} matches {    -- 8. Loss of control
										value matches {
											0.0|[local::at0100],
											1.0|[local::at0101],
											2.0|[local::at0102],
											3.0|[local::at0103],
											4.0|[local::at0104]
										}
									}
									ELEMENT[at0022] occurrences matches {0..1} matches {    -- 9. Not distressed
										value matches {
											4.0|[local::at0075],
											3.0|[local::at0076],
											2.0|[local::at0077],
											1.0|[local::at0078],
											0.0|[local::at0079]
										}
									}
									ELEMENT[at0023] occurrences matches {0..1} matches {    -- 10. Room cleanliness
										value matches {
											4.0|[local::at0080],
											3.0|[local::at0081],
											2.0|[local::at0082],
											1.0|[local::at0083],
											0.0|[local::at0084]
										}
									}
									ELEMENT[at0045] occurrences matches {0..1} matches {    -- Stress experienced during labour
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..16|}
											}
										}
									}
									ELEMENT[at0046] occurrences matches {0..1} matches {    -- Women’s personal attributes
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..8|}
											}
										}
									}
									ELEMENT[at0047] occurrences matches {0..1} matches {    -- Quality of care provision
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..16|}
											}
										}
									}
									ELEMENT[at0048] occurrences matches {0..1} matches {    -- Total score
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..40|}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0039] matches {*}    -- Item tree
		}
	}


