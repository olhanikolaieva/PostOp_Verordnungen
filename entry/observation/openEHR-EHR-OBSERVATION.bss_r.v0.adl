				["at0010"] = <
					text = <"2. Long labour">
					description = <"2. I thought my labour was excessively long.">
				>
				["at0016"] = <
					text = <"3. Feel in charge of labour">
					description = <"3. The delivery room staff encouraged me to make decisions about how I wanted my birth to progress.">
				>
				["at0017"] = <
					text = <"4. Anxiety">
					description = <"4. I felt very anxious during my labour and birth.">
				>
