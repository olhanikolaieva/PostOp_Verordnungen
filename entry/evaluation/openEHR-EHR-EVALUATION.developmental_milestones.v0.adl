definition
	EVALUATION[at0000] matches {    -- Developmental milestone summary
		data matches {
			ITEM_TREE[at0001] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0032] occurrences matches {0..1} matches {    -- Overall status
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0033,    -- Up-to-date for age
									at0034]    -- Not up-to-date for age
								}
							}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0002] occurrences matches {0..1} matches {    -- Overall description
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0024] occurrences matches {0..1} matches {    -- Overall trajectory
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0025,    -- Typical
									at0026,    -- Acute/Temporary
									at0027,    -- Delayed
									at0048,    -- Permanent impairment
									at0029,    -- Plateau/Lack of progress
									at0030]    -- Indeterminate
								}
							}
						}
					}
					CLUSTER[at0003] occurrences matches {0..*} matches {    -- Per milestone
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0004] matches {    -- Milestone name
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0010] occurrences matches {0..1} matches {    -- Category
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0011,    -- Physical - gross motor
											at0012,    -- Physical - fine motor
											at0013,    -- Cognitive
											at0014,    -- Language
											at0015,    -- Social
											at0016]    -- Emotional
										}
									}
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0035] occurrences matches {0..1} matches {    -- Trajectory
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0056,    -- Typical
											at0057,    -- Acute/Temporary
											at0058,    -- Delayed
											at0059,    -- Permanent impairment
											at0060,    -- Plateau/Lack of progress
											at0061]    -- Indeterminate
										}
									}
								}
							}
							ELEMENT[at0017] occurrences matches {0..1} matches {    -- Status
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0018,    -- Achieved
											at0047,    -- Reported as achieved
											at0019]    -- Not achieved
										}
									}
								}
							}
							ELEMENT[at0005] occurrences matches {0..1} matches {    -- Date achieved
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0043] occurrences matches {0..1} matches {    -- Date of assertion
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0022] occurrences matches {0..1} matches {    -- Variation
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0023] occurrences matches {0..*} matches {    -- Reason for delay
								value matches {
									DV_CODED_TEXT matches {*}
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0031] occurrences matches {0..1} matches {    -- Reason for regression
								value matches {
									DV_CODED_TEXT matches {*}
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0046] occurrences matches {0..1} matches {    -- Additional details
								include
									archetype_id/value matches {/.*/}
							}
							ELEMENT[at0006] occurrences matches {0..1} matches {    -- Comment
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					allow_archetype CLUSTER[at0045] occurrences matches {0..*} matches {    -- Additional details
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0044] occurrences matches {0..1} matches {    -- Overall comment
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0007] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0009] occurrences matches {0..1} matches {    -- Last updated
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					allow_archetype CLUSTER[at0008] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Developmental milestone summary">
					description = <"Summary information about the achievement of developmental milestones for an individual, usually a child, to monitor progress over time.">
				>
				["at0001"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Milestone name">
					description = <"Name of the developmental milestone.">
					comment = <"For example: walking independently, speaking first words, and using a pincer grasp to pick up small objects. Coding with an external terminology is recommended. Free text entry should only be permitted if no appropriate coded value is available. ">
				>
				["at0005"] = <
					text = <"Date achieved">
					description = <"The date when the milestone was reached.">
					comment = <"The younger the child, especially under 1 year, the greater the precision expected, but approximations are acceptable when this is the only information available and clearly identified as such in the health record, for example '~August 1, 2025'. For older children, or when information is recorded long after the event, partial dates such as month and year may be recorded. Where only an age is known, it should be converted to an approximate date and recorded using this data element.">
				>
				["at0006"] = <
					text = <"Comment">
					description = <"Additional narrative about the individual's developmental progress related to the identified milestone not captured in other fields.">
				>
				["at0007"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0003"] = <
					text = <"Per milestone">
					description = <"Details about each individual milestone.">
					comment = <"Record one instance of this cluster per milestone.">
				>
				["at0008"] = <
					text = <"Extension">
					description = <"Additional information required to extend the model with local content or to align with other reference models or formalisms.">
					comment = <"For example: local information requirements; or additional metadata to align with FHIR.">
				>
				["at0002"] = <
					text = <"Overall description">
					description = <"Narrative summary about the individual’s overall progress in achieving developmental milestones.">
				>
				["at0009"] = <
					text = <"Last updated">
					description = <"The date when the developmental milestones record was last updated.">
				>
				["at0010"] = <
					text = <"Category">
					description = <"Name of the developmental domain associated with the identified milestone.">
					comment = <"Each category is used to classify milestones into commonly recognised areas of child development and to support monitoring of progress at the domain level. Coding with an external terminology is recommended. Free text entry should only be permitted if no appropriate coded value is available. ">
				>
				["at0011"] = <
					text = <"Physical - gross motor">
					description = <"">
				>
				["at0012"] = <
					text = <"Physical - fine motor">
					description = <"">
				>
				["at0013"] = <
					text = <"Cognitive">
					description = <"">
				>
				["at0014"] = <
					text = <"Language">
					description = <"">
				>
				["at0015"] = <
					text = <"Social">
					description = <"">
				>
				["at0016"] = <
					text = <"Emotional">
					description = <"">
				>
				["at0017"] = <
					text = <"Status">
					description = <"Statement describing the current level of achievement for the identified milestone.">
					comment = <"\"Reported as achieved\" may be used to support documentation by carers or others when the milestone achievement is entered into a citizen's health record but cannot be directly witnessed or verified by a clinician. Coding with an external terminology is recommended. Free text entry should only be permitted if no appropriate coded value is available. ">
				>
				["at0018"] = <
					text = <"Achieved">
					description = <"The milestone has been completed and either directly observed or reliably confirmed by a healthcare professional.">
				>
				["at0019"] = <
					text = <"Not achieved">
					description = <"The milestone has not yet been completed or demonstrated by the individual.">
				>
				["at0022"] = <
					text = <"Variation">
					description = <"Narrative description about the deviation from normal development.">
					comment = <"For example, include commentary about the child belly crawling instead of typical hands-and-knees crawling.">
				>
				["at0023"] = <
					text = <"Reason for delay">
					description = <"Identification of the reason for delay in achievement of milestones.">
					comment = <"For example: prematurity, Down syndrome, recurrent ear infections, Autism Spectrum Disorder (ASD). Coding with an external terminology is recommended. Free text entry should only be permitted if no appropriate coded value is available. ">
				>
				["at0024"] = <
					text = <"Overall trajectory">
					description = <"The observed pattern or direction of an individual's developmental progress over time, relative to age-based expectations.">
					comment = <"Coding with an external terminology is recommended. Free text entry should only be permitted if no appropriate coded value is available. ">
				>
				["at0025"] = <
					text = <"Typical">
					description = <"The individual's developmental progress is consistent with what is expected for their age.">
				>
				["at0026"] = <
					text = <"Acute/Temporary">
					description = <"The individual's developmental progress has changed in the short term, usually in response to a transient condition such as illness, injury, hospitalisation, or psychosocial stressors, but is expected to improve over time.">
				>
				["at0027"] = <
					text = <"Delayed">
					description = <"The individual's developmental progress is behind what is expected for their age, but is expected to improve over time.">
				>
				["at0029"] = <
					text = <"Plateau/Lack of progress">
					description = <"The individual's developmental progress has stalled, but is expected to improve over time.">
				>
				["at0030"] = <
					text = <"Indeterminate">
					description = <"The individual's developmental trajectory cannot be determined due to insufficient, unclear, or conflicting information.">
				>
				["at0031"] = <
					text = <"Reason for regression">
					description = <"Identification of the reason for regression in achievement of milestones.">
					comment = <"For example: emotional neglect; neurodegenerative disease, hearing loss, or Autism Spectrum Disorder (ASD). Coding with an external terminology is recommended. Free text entry should only be permitted if no appropriate coded value is available. ">
				>
				["at0032"] = <
					text = <"Overall status">
					description = <"Statement describing the current level of achievement across all age-appropriate milestones.">
				>
				["at0033"] = <
					text = <"Up-to-date for age">
					description = <"The individual's developmental milestone achievements are consistent with what is expected for their age.">
				>
				["at0034"] = <
					text = <"Not up-to-date for age">
					description = <"The individual's developmental milestone achievements are not consistent with what is expected for their age.">
				>
				["at0035"] = <
					text = <"Trajectory">
					description = <"The observed pattern or direction of an individual's developmental progress over time, related to an identified milestone and relative to age-based expectations.">
					comment = <"Coding with an external terminology is recommended. Free text entry should only be permitted if no appropriate coded value is available. ">
				>
				["at0043"] = <
					text = <"Date of assertion">
					description = <"The date when the milestone was assessed.">
					comment = <"The date on which the 'Status' for the identified 'Milestone name' is documented.">
				>
				["at0044"] = <
					text = <"Overall comment">
					description = <"Additional narrative about the individual's overall developmental progress not captured in other fields.">
				>
				["at0045"] = <
					text = <"Additional details">
					description = <"Additional structured details about all milestone summary.">
				>
				["at0046"] = <
					text = <"Additional details">
					description = <"Additional structured details about the identified milestone.">
				>
				["at0047"] = <
					text = <"Reported as achieved">
					description = <"The milestone has been completed based on information provided by a parent, caregiver, or other observer, but has not been directly observed by a healthcare professional.">
				>
				["at0048"] = <
					text = <"Permanent impairment">
					description = <"The individual's developmental progress is behind what is expected for their age and is not expected to improve over time.">
				>
				["at0056"] = <
					text = <"Typical">
					description = <"The individual's developmental progress is consistent with what is expected for their age.">
				>
				["at0057"] = <
					text = <"Acute/Temporary">
					description = <"The individual's developmental progress has changed in the short term, usually in response to a transient condition such as illness, injury, hospitalisation, or psychosocial stressors, but is expected to improve over time.">
				>
				["at0058"] = <
					text = <"Delayed">
					description = <"The individual's developmental progress is behind what is expected for their age, but is expected to improve over time.">
				>
				["at0059"] = <
					text = <"Permanent impairment">
					description = <"The individual's developmental progress is behind what is expected for their age and is not expected to improve over time.">
				>
				["at0060"] = <
					text = <"Plateau/Lack of progress">
					description = <"The individual's developmental progress has stalled, but is expected to improve over time.">
				>
				["at0061"] = <
					text = <"Indeterminate">
					description = <"The individual's developmental trajectory cannot be determined due to insufficient, unclear, or conflicting information.">
				>
			>
		>
	>
